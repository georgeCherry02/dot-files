{
    "clangd.fallbackFlags": [
        "-std=c++17",
        "-Wall",
        "-Wextra",
        "-Wpedantic",
        "-isystem",
        "/opt/bb/include",
        "-I."
    ],
    "coc.preferences.semanticTokensHighlights": true,
    "cmake.cmakePath": "/opt/bb/bin/bbcmake",
    "cmake.formatter": "~/.local/bin/cmake-format",
    "cmake.formatter_args": [
        "-c",
        "~/.config/cmake_format.py"
    ],
    "http.proxyStrictSSL": false,
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.local/bin/black",
    "python.formatting.blackArgs": [
        "-t",
        "py39"
    ],
    "coc.preferences.jumpCommand": "tabe",
    "coc.preferences.snippetHighlight": true,
    "rust-analyzer.serverPath": "~/.local/bin/rust-analyzer"
}
